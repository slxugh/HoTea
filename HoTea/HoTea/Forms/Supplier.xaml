<Window x:Name="SupplierForm" x:Class="lab9.Supplier"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:lab9"
        mc:Ignorable="d"
        Background="#fcf8f4"
        Title="Редактирование поставщика" Height="255" Width="354" ShowInTaskbar="False" ResizeMode="CanMinimize" FontFamily="SF Pro Display" FontSize="14">
    <Grid Margin="0,0,0,-6">
        <Label Content="Код поставщика" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>
        <Label Content="Название" HorizontalAlignment="Left" Margin="10,47,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.523,0.738"/>
        <Label Content="Страна" HorizontalAlignment="Left" Margin="10,93,0,0" VerticalAlignment="Top"/>
        <Label Content="Телефон" HorizontalAlignment="Left" Margin="6,131,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="tbNameSupplier" HorizontalAlignment="Left" Margin="130,45,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="189" Height="30"/>
        <TextBox x:Name="tbCountry" HorizontalAlignment="Left" Margin="130,91,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="189" Height="30"/>
        <TextBox x:Name="tbPhone" HorizontalAlignment="Left" Margin="130,131,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="189" TextChanged="tbPhone_TextChanged" PreviewTextInput="tbPhone_PreviewTextInput" KeyDown="tbPhone_KeyDown" Height="30"/>
        <Label x:Name="labelSupplierID" Content="Авто" HorizontalAlignment="Left" Margin="130,12,0,0" VerticalAlignment="Top" Height="30" Width="189" FontFamily="SF Pro Display" FontSize="14"/>
        <Image x:Name="btnSaveInSupplier" Height="42" Margin="10,170,0,0" VerticalAlignment="Top" Source="/Assets/Pics/save.png" RenderTransformOrigin="0.5,0.5" HorizontalAlignment="Left" Width="40" MouseDown="btnSaveInSupplier_MouseDown">
            <Image.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform Angle="0"/>
                    <TranslateTransform/>
                </TransformGroup>
            </Image.RenderTransform>
        </Image>
    </Grid>
</Window>
