<Window x:Name="TypeTea" x:Class="lab9.TypesTea"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:lab9"
        mc:Ignorable="d"
        Background="#fcf8f4"
        Title="Редактирование типа чая" Height="158" Width="337" ShowInTaskbar="False" ResizeMode="CanMinimize" FontFamily="SF Pro Display" FontSize="14">
    <Grid Margin="0,0,0,-6">
        <Label Content="Код типа чая" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>
        <Label Content="Название" HorizontalAlignment="Left" Margin="10,46,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="tbTypesTeaName" HorizontalAlignment="Left" Margin="108,45,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="189" Height="29"/>
        <Label x:Name="labelTypeTeaID" Content="Авто" HorizontalAlignment="Left" Margin="108,10,0,0" VerticalAlignment="Top" Height="23" Width="189" FontFamily="SF Pro Display" FontSize="14"/>
        <Image x:Name="btnSaveInSupplier" Height="42" Margin="10,78,0,0" VerticalAlignment="Top" Source="/Assets/Pics/save.png" RenderTransformOrigin="0.55,0.66" HorizontalAlignment="Left" Width="40" MouseDown="btnSaveInSupplier_MouseDown">
            <Image.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform Angle="0"/>
                    <TranslateTransform/>
                </TransformGroup>
            </Image.RenderTransform>
        </Image>
    </Grid>
</Window>
