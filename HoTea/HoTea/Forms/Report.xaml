<Window x:Name="ReportForm" x:Class="lab9.Forms.Report"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:lab9.Forms"
        mc:Ignorable="d"
        Title="Создание отчета по продажам" Height="150" Width="388"
        Background="#fcf8f4" FontFamily="SF Pro Display" FontSize="14" ResizeMode="CanMinimize">
    <Grid>
        <DatePicker x:Name="startDatePicker" Width="195" Margin="132,11,0,0" Height="26" VerticalAlignment="Top" HorizontalAlignment="Left"/>
        <Label Content="Дата начала" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="117"/>
        <Label Content="Дата окончания" HorizontalAlignment="Left" Margin="10,42,0,0" VerticalAlignment="Top" Width="117"/>
        <Image x:Name="btnSave" HorizontalAlignment="Left" Height="32" Margin="10,74,0,0" VerticalAlignment="Top" Width="34" Source="/Assets/Pics/save.png" MouseDown="btnSave_MouseDown"/>
        <DatePicker x:Name="endDatePicker" Width="196" Margin="132,43,0,0" Height="26" VerticalAlignment="Top" HorizontalAlignment="Left"/>
    </Grid>
</Window>
